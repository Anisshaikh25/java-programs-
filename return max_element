
import java.util.* ;
import java.io.*; 

public class Solution {

    static int largestElement(int[] arr, int n) {
        // Write your code here.
        int max=arr[0];

        for(int i:arr)
        {
            if(i>max)
            {
                max=i;
            }
        }
        return max;

    }
}
